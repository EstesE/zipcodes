{
    "application": {
        "name": "zipcodes"
    },
    "url": "https://tools.usps.com/tools/app/ziplookup/cityByZip",
    "timeout": 3000,
    "shuffle": true,
    "data": {
        "save": true,
        "connection": "mongodb://localhost:27017/zipcodes"
    },
    "geocoding": {
        "baseApiURL": "https://maps.googleapis.com/maps/api/geocode/json?address="
    },
    "elevation": {
        "baseApiURL": "https://maps.googleapis.com/maps/api/elevation/json?locations="
    },
    "timezone": {
        "baseApiURL": "https://maps.googleapis.com/maps/api/timezone/json?location="
    }
}